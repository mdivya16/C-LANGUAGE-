
#include <stdio.h>

int main() {
    char ch;
    char password[20];
    int i = 0;

    printf("Enter any password: ");

    while (i < 19 && (ch = getchar()) != '\n' && ch != EOF) {
        password[i] = ch;
        i++;
        printf(" * ");
    }

    password[i] = '\0'; // Null-terminate the string

    printf("\nYou entered the password: %s\n", password);

    return 0;
}
